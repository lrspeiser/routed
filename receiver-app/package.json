{
  "name": "routed",
  "version": "0.1.0",
  "private": true,
  "author": "Routed",
  "description": "Routed desktop receiver",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "dist": "electron-builder --mac dmg"
  },
  "build": {
    "appId": "com.routed.app",
    "productName": "Routed",
    "files": [
      "main.js",
      "preload.js",
      "renderer.html",
      "package.json",
      "arrow-icon-routed.png"
    ],
    "icon": "build/icon.icns",
    "mac": {
      "category": "public.app-category.utilities",
      "target": ["dmg"],
      "identity": null,
      "icon": "build/icon.icns"
    },
    "dmg": {
      "icon": "build/icon.icns"
    }
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  }
}
